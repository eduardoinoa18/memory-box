{
  "version": 2,
  "name": "memory-box-admin",
  "builds": [
    {
      "src": "admin-dashboard/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "admin-dashboard/$1"
    },
    {
      "src": "/(.*)",
      "dest": "admin-dashboard/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "admin-dashboard/pages/api/**/*.js": {
      "maxDuration": 30
    }
  }
}
